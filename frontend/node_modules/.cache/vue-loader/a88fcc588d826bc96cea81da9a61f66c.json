\"\r\n              :rules=\"rules.animal\"\r\n              color=\"pink\"\r\n              label=\"카테고리\"\r\n              required\r\n            ></v-select>\r\n          </v-col>\r\n          <v-col cols=\"12\" sm=\"6\">\r\n            <v-text-field\r\n              v-model=\"form.first\"\r\n              :rules=\"rules.name\"\r\n              color=\"purple darken-2\"\r\n              label=\"제목\"\r\n              required\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\">\r\n            <v-textarea\r\n              v-model=\"form.bio\"\r\n              color=\"teal\"\r\n            >\r\n              <template v-slot:label>\r\n                <div>\r\n                  POST\r\n                </div>\r\n              </template>\r\n            </v-textarea>\r\n          </v-col>\r\n          \r\n          <!-- <v-col cols=\"12\" sm=\"6\">\r\n            <v-slider\r\n              v-model=\"form.age\"\r\n              :rules=\"rules.age\"\r\n              color=\"orange\"\r\n              label=\"Age\"\r\n              hint=\"Be honest\"\r\n              min=\"1\"\r\n              max=\"100\"\r\n              thumb-label\r\n            ></v-slider>\r\n          </v-col> -->\r\n          <v-btn class=\"ma-2\" right=\"true\" outlined color=\"indigo\">등록</v-btn>\r\n        </v-row>\r\n      </v-container>\r\n    </v-form>\r\n  </v-card>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data () {\r\n      const defaultForm = Object.freeze({\r\n        first: '',\r\n        last: '',\r\n        bio: '',\r\n        favoriteAnimal: '',\r\n        age: null,\r\n        terms: false,\r\n      })\r\n\r\n      return {\r\n        form: Object.assign({}, defaultForm),\r\n        rules: {\r\n          age: [\r\n            val => val < 10 || `I don't believe you!`,\r\n          ],\r\n          animal: [val => (val || '').length > 0 || 'This field is required'],\r\n          name: [val => (val || '').length > 0 || '